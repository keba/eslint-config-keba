{
    "name": "eslint-config-keba",
    "version": "1.3.0",
    "description": "An ESLint config alternative - don't use if you don't like it...",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/keba/eslint-config-keba.git"
    },
    "author": "keba",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/keba/eslint-config-keba/issues"
    },
    "homepage": "https://github.com/keba/eslint-config-keba#readme",
    "dependencies": {
        "babel-eslint": ">=8.2.5",
        "eslint": ">=5.0.1"
    },
    "peerDependencies": {
        "babel-eslint": ">=8.2.5",
        "eslint": ">=5.0.1"
    },
    "scripts": {
        "eslint": "./node_modules/eslint/bin/eslint.js -c package.json **/*.js",
        "eslintfix": "./node_modules/eslint/bin/eslint.js --fix -c package.json **/*.js",
        "test": "npm run eslint"
    },
    "keywords": [
        "eslint",
        "eslintconfig"
    ],
    "engines": {
        "node": ">=8.10.0"
    },
    "eslintConfig": {
        "extends": "./index.js"
    },
    "eslintIgnore": [
        "**/*.json",
        "**/*.sql",
        "**/*.log"
    ]
}
